{
  "version": 1,
  "projects": {
    "covid": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/covid",
      "sourceRoot": "apps/covid/src",
      "prefix": "covid-app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/covid",
            "index": "apps/covid/src/index.html",
            "main": "apps/covid/src/main.ts",
            "polyfills": "apps/covid/src/polyfills.ts",
            "tsConfig": "apps/covid/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/covid/src/favicon.ico", "apps/covid/src/assets"],
            "styles": ["apps/covid/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/covid/src/environments/environment.ts",
                  "with": "apps/covid/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "covid:build",
            "proxyConfig": "apps/covid/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "covid:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "covid:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/covid/tsconfig.app.json",
              "apps/covid/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/covid/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/covid/jest.config.js",
            "tsConfig": "apps/covid/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/covid/src/test-setup.ts"
          }
        }
      }
    },
    "covid-e2e": {
      "root": "apps/covid-e2e",
      "sourceRoot": "apps/covid-e2e/src",
      "projectType": "application",
      "architect": {
